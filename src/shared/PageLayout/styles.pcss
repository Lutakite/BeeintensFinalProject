@import "styles/constants.pcss";

.container {
  display: flex;
  min-height: 100vh;
  min-width: 320px;
  align-items: stretch;
  flex-direction: column;
  background: var(--backgroundColor);

}

.containerIn {
  display: flex;
  flex: 1 1 auto;
  justify-content: stretch;
  flex-direction: column;
  margin: 0 auto;
  max-width: 1500px;

}

.content {
  flex: 1 1 auto;
  max-width: 960px;
  margin: 0 auto;
}



.afterContent { flex: 1 1 auto; }
